              img(width='34', height='34', src='images/avatar-male.jpg')
              =user.name
              b.caret
            ul.dropdown-menu
              li
                a(href='#')
                  i.icon-user
                  | My Account
              li
                a(href='#')
                  i.icon-gear
                  | Account Settings
              li
